<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="category">

 <select id="category_one_c_name" parameterType="String" resultType="vo.CategoryVo">
	select * from categories where c_name = #{ c_name }
 </select>
 
  <select id="category_one_c_idx" parameterType="int" resultType="vo.CategoryVo">
	select * from categories where c_idx = #{ c_idx }
 </select>
  

</mapper>












